      <h1><u>WTF NFT purchases!</u></h1>
      
      <br>
      {{#each item}}
        <div class="cube">
          <img class="cube" src={{imageUrl}}>
          <p class="name">{{name}}</p>
          <p><span>Category:</span>{{category}}</p>
          <a class="btn" href="/details/{{_id}}">Details</a>

        </div>
      
      {{else}}
        <h2> No Items found</h2>
      {{/each}}
      
      <br>
      <p>
      <form action="/search" method="GET">
        <input type="text" class="search" name="search" placeholder="Search by category...">
        <input type="submit" value="search">
      </form></p>